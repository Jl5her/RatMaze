{
  "name": "ratrace",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "serve": "node build/app.js",
    "dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "NODE_ENV=development ts-node --files src/index.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "^4.0.0",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.0.3",
    "typescript": "^5.3.3"
  }
}
